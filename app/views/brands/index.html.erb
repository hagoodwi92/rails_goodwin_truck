<table class="table table-striped">
  <tr>
    <th><h1>Brands</h1></th>
  </tr>
  <% if @brands.any? %>
<% @brands.each do |brand| %>
  <tr>
    <td><%= link_to brand.name, brand_path(brand) %></td></br>
  </tr>
  <% end %>
    <% else %>
    <p>There are no brands yet.</p>
    <% end %>
    </br>

    </table>
</br>
  <h4><%= link_to "Create new brand", new_brand_path %></h4>